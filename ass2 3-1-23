n=int(input())
a=[]
s1=0
s2=0
for i in range(0,n):
    l=[]
    for j in range(0,n):
        x=int(input())
        l.append(x)
    a.append(l)
for i in a:
    print(i)
for i in range(n):
    for j in range(n):
        if i==j:
            s1=s1+a[i][j]
        if i+j==n-1:
            s2=s2+a[i][j]
print(s1-s2)            
    
